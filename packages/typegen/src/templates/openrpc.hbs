{
  "openrpc": "1.2.6",
  "info": {
    "version": "1.0.0",
    "title": "Frequency API"
  },
  "methods": [
{{#each allRPCMethods}}
    {
      "name": "{{name}}",
      "params": [
{{#each params}}
        {
          "name": "{{name}}",
          "description": "",
          "required": {{required}},
          "schema": {{{schema}}}
        }{{#unless @last}},{{/unless}}
{{/each}}
      ]
    }{{#unless @last}},{{/unless}}
{{/each}}
 ]
}

