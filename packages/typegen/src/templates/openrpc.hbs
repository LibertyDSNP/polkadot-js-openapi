{
  "openrpc": "1.2.6",
  "info": {
    "version": "1.0.0",
    "title": "Frequency API"
  },
  "methods": [
{{#each methods}}
    {
      "name": "{{name}}",
      "params": [
{{#each params}}
        {
          "name": "{{name}}",
          "description": "",
          "required": {{required}},
          "schema": {{{json schema}}}
        }{{#unless @last}},{{/unless}}
{{/each}}
      ],
      "tags": [
{{#each tags}}
{
  "name": "{{name}}"
}{{#unless @last}},{{/unless}}
{{/each}}
      ],
      "result": {
        "name": "result",
        "schema": {

        }
      }
    },
{{/each}}
{{#each extrinsics}}
  {
    "name": "{{palletName}}_{{method.name}}",
    "params": [
      {{#each method.params}}
        {
          "name": "{{name}}",
          "description": "",
          "required": {{required}},
          "schema": {{{json type}}}
        }{{#unless @last}},{{/unless}}
      {{/each}}
    ]
  }{{#unless @last}},{{/unless}}
  {{/each}}
 ],
 "components": {
  "schemas":
    {{{jsonmap schemas}}}
 }
}

